<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="904.0" prefWidth="1203.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tecinfo.poo.controller.SecondaryEditController">
    <children>
      <TableView fx:id="tbvPlayers" prefHeight="400.0" prefWidth="1163.0" />
      <Button fx:id="flushButton" mnemonicParsing="false" onAction="#flushTable" text="Deleter Conteúdos da Tabela" />
        <Label fx:id="lblCounter" text="Secondary View">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
        <Button fx:id="secondaryButton" onAction="#switchToThird" text="Switch to Third" />
      <Label fx:id="winAnnouncer" text="Máquina Venceu!">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
